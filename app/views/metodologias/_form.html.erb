<%= simple_form_for(@metodologia) do |f| %>
  <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :descripcion %>
      <%= f.text_area :descripcion %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
<% end %>
