<%= form_for(@subproducto, :html => { :multipart => true }) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :descripcion %>
      <%= f.text_area :descripcion %>

      <%= f.label :imagen %>
      <%= f.file_field :photo %>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
